{
    "name": "yanzicms/jpwrt",
    "type": "project",
    "description": "Jpwrt developed based on the Jsnpp framework.",
    "keywords": [
        "jpwrt",
        "blog",
        "website"
    ],
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/yanzicms/jpwrt/issues",
        "source": "https://github.com/yanzicms/jpwrt"
    },
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "yanzicms/jsnpp": "^4.0",
        "phpmailer/phpmailer": "^6.2"
    },
    "require-dev": {
        "yanzicms/jsnpp": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/",
            "jpwrt\\theme\\": "public/themes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
